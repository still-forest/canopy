{
  "name": "@still-forest/canopy",
  "version": "0.52.0",
  "author": "John Szymanowski",
  "license": "MIT",
  "description": "React components, built with Tailwind CSS, Shadcn, and VisX",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/still-forest/canopy.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "dev": "vite",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:ci": "biome ci . && tsc --noEmit",
    "lint:fix": "biome check --write --unsafe .",
    "prepublishOnly": "pnpm build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:build:docs": "storybook build --docs",
    "storybook:docs": "storybook dev --docs",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run"
  },
  "dependencies": {
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.563.0",
    "radix-ui": "^1.4.3",
    "react-day-picker": "^9.9.0",
    "react-router": "^7.8.2",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@still-forest/biomejs-config": "2.2.0",
    "@storybook/addon-docs": "10.2.1",
    "@storybook/addon-themes": "10.2.1",
    "@storybook/builder-vite": "10.2.1",
    "@storybook/react-vite": "10.2.1",
    "@tailwindcss/vite": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.10.9",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.3",
    "@vitest/browser": "4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "jsdom": "27.4.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "release-it": "19.2.4",
    "storybook": "10.2.1",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vitest": "4.0.18"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.0.0",
    "tw-animate-css": "^1.0.0"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "overrides": {
      "rollup": "4.57.0"
    }
  }
}
