{
  "name": "@still-forest/canopy",
  "version": "0.22.0",
  "author": "John Szymanowski",
  "license": "MIT",
  "description": "React components, built with Tailwind CSS, Shadcn, and VisX",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/still-forest/canopy.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "dev": "vite",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:ci": "biome ci .",
    "lint:fix": "biome check --write --unsafe .",
    "prepublishOnly": "pnpm build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:build:docs": "storybook build --docs",
    "storybook:docs": "storybook dev --docs",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run"
  },
  "dependencies": {
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.525.0",
    "radix-ui": "^1.4.2",
    "react-day-picker": "^9.6.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.4",
    "@still-forest/biomejs-config": "2.0.0",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/addon-themes": "9.0.16",
    "@storybook/builder-vite": "9.0.16",
    "@storybook/react-vite": "9.0.16",
    "@tailwindcss/vite": "4.1.11",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.15.33",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.6.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "chromatic": "12.2.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "jsdom": "26.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "release-it": "19.0.3",
    "storybook": "9.0.16",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.11",
    "tw-animate-css": "1.3.5",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.0.0"
  },
  "packageManager": "pnpm@10.13.1"
}
